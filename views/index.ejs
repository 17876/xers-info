<!DOCTYPE html>
<html lang=<%= database.language %>>

<% let title = "ANTON X - Home"%>
<%- include('./partials/head.ejs', { title: title}) %>

<body>
  <div class="wrapper">
    <%- include('./partials/header.ejs', { database: database }) %>
    <div id="main-content-div">
      <%= database.shortabout %>
      <div id="projects-content">
        <div class="projects-flex-container">
          <% for (var [key, value] of Object.entries(database.projects)) { %>
          <%- include('./partials/project-thumbnail.ejs', {language: database.language, project: value}) %>
          <% } %>
        </div>
      </div>
    </div>
  </div>
  <%- include('./partials/footer.ejs', { database: database }) %>
  </div>
</body>

</html>