<div class="project-meta">
  <%-project_meta.title%>
  <% for (var i = 0; i < project_meta.items.length; i++) {
  if (project_meta.items[i]) {%>
    <% if (project_meta.items[i].label!=0) {%>
    <div class="inline-wrapper">
    <span><%-project_meta.items[i].label%></span>
    <% } 

      for (var j = 0; j<project_meta.items[i].value.length; j++) {
        let aclass = '';
        if (j!=project_meta.items[i].value.length-1) {
          aclass = 'with-comma';
          }

        if (project_meta.items[i].value[j]) { %>
    <span class=<%=aclass%>><%-project_meta.items[i].value[j]%></span>
      </div>
    <% }
          else { %>
          div class="inline-wrapper">
    <span class=<%=aclass%>><%-project_meta.items[i].value[j]%></span>
    </div>
    <% }
      } %>
  <% }
  else { %>
    <%-project_meta.items[i].label%> <%-project_meta.items[i].value%>
  <% } 
    }%>
</div>