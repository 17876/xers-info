<img id="overlay-img" src="<%= overlay_nav.img_src %>">
<svg id="Layer_1" preserveAspectRatio="xMidYMid slice" xmlns="http://www.w3.org/2000/svg" version="1.1" viewBox="0 0 1920 1259">
  <% for (var i = 0; i<overlay_nav.overlay_links.length; i++) {
    const overlay_link = overlay_nav.overlay_links[i];
    const curGroupId = overlay_link.id;
    const curDynamicGroupClass = `hover-group-${curGroupId}`;
    const curDynamicCaptionClass = `overlay-caption-link-${curGroupId}`;
    const curDynamicCaptionLangClass = `overlay-caption-${language}`;
  if (overlay_link.src != "") { %>
    <a href="/<%= language %><%= overlay_link.src %>">
    <%}%>
      <g id="<%= curGroupId %>" class="hover-group <%= curDynamicGroupClass %>" >
        <% for (var j = 0; j<overlay_link.paths.length; j++) { 
          const curPath = overlay_link.paths[j];
          const curDynamicPathClass = `overlay-path-${curGroupId}`;
          %>
        <polygon class="overlay-path <%= curDynamicPathClass %>" points="<%= curPath.points %>" />
        <%}%>
        <text class="overlay-caption <%= curDynamicCaptionLangClass %> <%= curDynamicCaptionClass %>" ><%= overlay_link.caption %></text>
      </g>
    <% if (overlay_link.src != "") {%>
    </a>
    <% }
  } %>
</svg>