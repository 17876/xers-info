<!DOCTYPE html>
<html lang=<%= database.lang %>>

<% let title = `ANTON X - ${database.imprint.title}`%>
<%- include('./partials/head.ejs', {title: title}) %>

<body>
  <div class="wrapper">
    <%- include('./partials/header.ejs', { database: database }) %>
    <div class="imprint-title">
      <%- database.imprint.title %>
    </div>
    <div class="flex-container">
      <div class="imprint">
        <%- database.imprint.text %>
      </div>
    </div>
    <%- include('./partials/footer.ejs', { database: database }) %>
  </div>
</body>

</html>